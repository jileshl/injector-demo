<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true" android:state_pressed="true"
        android:drawable="@drawable/ic_indicator_tab_selected_pressed" />
    <item android:state_checked="false" android:state_pressed="true"
        android:drawable="@drawable/ic_indicator_tab_unselected_pressed" />

    <item android:state_checked="false"
        android:drawable="@drawable/ic_indicator_tab_unselected" />
    <item android:state_checked="true"
        android:drawable="@drawable/ic_indicator_tab_selected" />
</selector>